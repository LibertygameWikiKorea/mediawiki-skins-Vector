{
	"root": true,
	"plugins": [
		"no-jquery"
	],
	"extends": [
		"plugin:no-jquery/deprecated",
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"exports": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"settings": {
	},
	"rules": {
		"mediawiki/no-nodelist-unsupported-methods": "off",
		"jsdoc/no-undefined-types": "off",
		"compat/compat": "off",
		"mediawiki/class-doc": "off"
	}
}
